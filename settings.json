{
    "editor.bracketPairColorization.enabled": false,
    "editor.fontFamily": "Hack",
    "editor.fontSize": 24,
    "terminal.integrated.fontSize": 24,
    "editor.lineHeight": 32,
    "workbench.colorTheme": "DarkRoy 2",
    "editor.cursorStyle": "block",
    "editor.cursorBlinking": "solid",
    "window.menuBarVisibility": "hidden",
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.scrollbar.horizontalScrollbarSize": 0,
    "workbench.activityBar.location": "hidden",
    "terminal.integrated.inheritEnv": true,
    "terminal.integrated.tabs.enabled": true,
    "terminal.external.linuxExec": "konsole",
    "workbench.startupEditor": "none",
    "git.openRepositoryInParentFolders": "always",
    "window.density.editorTabHeight": "compact",
    "apc.parts.font.family": {
        "sidebar": "Hack"
    },
    "apc.listRow": {
        "fontSize": 24,
        "height": 32
    },
    "apc.stylesheet": {
        ".monaco-workbench .part.panel.bottom .composite.title ": "height: 0px",
        ".monaco-icon-label>.monaco-icon-label-container": "overflow-x: clip; overflow-y: visible;",
        ".show-file-icons .file-icon::before": "min-width: fit-content;"
    },
    "workbench.colorCustomizations": {
        "editorOverviewRuler.border": "#00000000",
        "panelTitle.activeBorder": "#ffffff"
    },
    "extensions.autoUpdate": false,
    "update.mode": "none",
    "apc.sidebar.titlebar": {
        "width": 250
    }
}